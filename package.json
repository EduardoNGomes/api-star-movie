{
  "name": "films-fans-project",
  "version": "1.0.0",
  "description": "A API Rocketseat made by Explorer program",
  "main": "index.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node build/app/index.js",
    "dev": "tsx --watch src/app/index.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "knex": "node --no-warnings --loader tsx ./node_modules/.bin/knex",
    "migrate-deploy": "npx knex --knexfile knexfile.ts migrate:latest ",
    "lint": "eslint src --ext .ts --fix",
    "build": "tsup src --out-dir build",
    "deploy": "npm i && npm run migrate-deploy && npm run build"
  },
  "author": "Eduardo Nascimento Gomes",
  "license": "ISC",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.1",
    "knex": "^2.5.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.1",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.46.0",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  }
}
