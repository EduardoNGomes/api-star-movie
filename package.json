{
  "name": "films-fans-project",
  "version": "1.0.0",
  "description": "A API Rocketseat made by Explorer program",
  "main": "index.ts",
  "scripts": {
    "start": "node src/app/index.ts",
    "dev": "tsx --watch src/app/index.ts",
    "test": "vitest run",
    "knex": "node --no-warnings --loader tsx ./node_modules/.bin/knex",
    "migrate-deploy": "npx knex --knexfile knexfile.ts migrate:latest ",
    "lint": "eslint src --ext .ts --fix",
    "build": "tsup src --out-dir build",
    "ci": "npm run migrate && npm start"
  },
  "author": "Eduardo Nascimento Gomes",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "knex": "^2.5.1",
    "pg": "^8.11.1",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.5",
    "eslint": "^8.46.0",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  }
}
